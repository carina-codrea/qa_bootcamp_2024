<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="PetStore" verbose="1">
    <listeners>
        <listener class-name="customReporting.CustomTestListener"/>
        <listener class-name="customReporting.CustomTestReporter"/>
    </listeners>
    <test name="PetTest">
        <classes>
            <class name="petStore.test.PetTest"/>
        </classes>
    </test>
    <test name="StoreTest">
        <classes>
            <class name="petStore.test.StoreTest"/>
        </classes>
    </test>
    <test name="UserTest">
        <classes>
            <class name="petStore.test.UserTest"/>
        </classes>
    </test>
</suite>